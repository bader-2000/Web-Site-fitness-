@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_LayoutHomePage.cshtml";
}
<section class="site-banner">
	<div class="banner-content">
		<div class="container">
			<div class="row">
				<div class=" col-md-12 text-content light">
					<h1>Contact Us</h1>
					<div class="breadcum-items">
						<span class="item"><a href="index.html">Home /</a></span>
						<span class="item colored">Contact Us</span>
					</div>
				</div><!--text-content-->
			</div>
		</div>
	</div>
</section>
<section class="contact-information padding-large bg-sand">
	<div class="container">
		<div class="row">
			<div class="col-md-6 p-0 mb-3">
				<div class="container">
					<div class="section-header text-center padding-small">
						<h2 class="section-title">Get in Touch</h2>
					</div>

					<p>We are consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>

					<div class="contact-detail mt-4">
						<div class="detail mr-6 mb-4">
							<i class="icon icon-phone mr-3"></i>
							<ul class="list-unstyled">
								<li>+1650-243-0000</li>
								<li>+1234-243-1111</li>
							</ul>
						</div><!--detail-->
						<div class="detail mb-4">
							<i class="icon icon-envelope mr-3"></i>
							<ul class="list-unstyled">
								<li><a href="mailto:hello@templatesjungle.com">hello@templatesjungle.com</a></li>
								<li><a href="mailto:info@templatesjungle.com">info@templatesjungle.com</a></li>
							</ul>
						</div><!--detail-->
						<div class="address detail mb-4">
							<i class="icon icon-map-marker mr-3"></i>
							<ul class="list-unstyled">
								<li>North Melbourne VIC 3051, Australia</li>
								<li>North Melbourne VIC 3051, Australia</li>
							</ul>
						</div><!--detail-->

					</div><!--contact-detail-->
				</div>
			</div><!--col-md-6-->

			<div class="col-md-6 p-0">
				<div class="container">
					<div class="row">
						<div class="section-header text-center padding-small">
							<h2 class="section-title">Get Appointment</h2>
						</div>
						<form class="contact-form d-flex flex-wrap mt-4">
							<div class="text-input flex-container">
								<input type="text" name="name" placeholder="Your Full Name" class="name mr-sm-0 mr-3 mb-3">
								<input type="text" name="E-mail" placeholder="Your E-mail Address" class="mb-3">
							</div><!--text-input-->
							<textarea placeholder="Your Message" style="width:100%; height: 150px;" spellcheck="false" data-ms-editor="true"></textarea>
						</form>
					</div>
				</div>
				<div class="Subscribe-button text-center light mt-4">
					<p class="colored-grey">*We protect your privacy</p>
					<button class="hvr-shutter-in-horizontal m-0">Send</button>
				</div>
			</div><!--col-md-6-->

		</div>
	</div>
</section>