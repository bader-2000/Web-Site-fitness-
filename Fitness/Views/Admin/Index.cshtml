                      <!-- Chart Card -->
                   <div class="col-12 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h5>Subscriptions Over Time</h5>
                        <div>
                            <canvas id="subscriptionsChart" style="max-width: 100%; height: 400px;"></canvas>
                        </div>
                    </div>
                </div>

                <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
                <script>
                    // الحصول على البيانات من ViewData
                    const chartData = @Html.Raw(ViewData["ChartData"]);

                    // تحويل البيانات إلى arrays
                    const labels = chartData.map(item => `Month ${item.Month}`); // الأشهر
                    const data = chartData.map(item => item.Subscriptions); // عدد الاشتراكات

                    // إعداد الرسم البياني
                    const ctx = document.getElementById('subscriptionsChart').getContext('2d');
                    new Chart(ctx, {
                        type: 'bar', // نوع المخطط
                        data: {
                            labels: labels, // الأشهر
                            datasets: [{
                                label: 'Number of Subscriptions', // عنوان الرسم البياني
                                data: data, // البيانات
                                backgroundColor: '#36a2eb', // لون الأعمدة
                                borderColor: '#fff',
                                borderWidth: 1,
                                barThickness: 40, // سمك الأعمدة
                                hoverBackgroundColor: '#4bc0c0', // لون عند التمرير
                            }]
                        },
                        options: {
                            responsive: true,
                            plugins: {
                                title: {
                                    display: true,
                                    text: 'Monthly Subscriptions',
                                    font: { size: 18 }
                                },
                                legend: {
                                    display: false
                                }
                            },
                            scales: {
                                x: { ticks: { font: { size: 12 } } },
                                y: { ticks: { font: { size: 12 } } }
                            }
                        }
                    });
                </script>

                   </div>
