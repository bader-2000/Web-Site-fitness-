@model IEnumerable<Fitness.Models.Typeperson >


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}





<!-- Required meta tags -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<title>Corona Admin</title>
<!-- plugins:css -->
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/mdi/css/materialdesignicons.min.css">
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/css/vendor.bundle.base.css">
<!-- endinject -->
<!-- Plugin css for this page -->
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap.css">
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/flag-icon-css/css/flag-icon.min.css">
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/owl-carousel-2/owl.carousel.min.css">
<link rel="stylesheet" href="~/DesignAdman/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
<!-- End plugin css for this page -->
<!-- inject:css -->
<!-- endinject -->
<!-- Layout styles -->
<link rel="stylesheet" href="~/DesignAdman/assets/css/style.css">
<!-- End layout styles -->
<link rel="shortcut icon" href="~/DesignAdman/assets/images/favicon.png" />



<div class="content-wrapper">
    <div class="main-panel">
        <div class="col-12 grid-margin">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title"> Search for subscriptions </h4>
                    <div class="table-responsive">


                        <div class="container">
                            <form asp-controller="Admin" asp-action="Search" method="post" class="mb-4">
                                <div class="row">
                                    <div class="col-md-4">
                                        <label for="StartDate">Start Date:</label>
                                        <input type="date" name="StartDate" class="form-control" />
                                    </div>
                                    <div class="col-md-4">
                                        <label for="EndDate">End Date:</label>
                                        <input type="date" name="EndDate" class="form-control" />
                                    </div>
                                    <div class="col-md-4 align-self-end">
                                        <button type="submit" class="btn btn-primary mt-2">Filter</button>
                                    </div>
                                </div>
                            </form>

                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Startdate)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Enddate)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Tsubscr.Price)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Tsubscr.Countweeks)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Tsubscr.Nameplan)
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Startdate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Enddate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tsubscr.Price)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tsubscr.Countweeks)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Tsubscr.Nameplan)
                                            </td>
                                            
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>



                            
                </div>

            </div>


        </div>
    </div>
</div>

<script src="~/DesignAdman/assets/vendors/js/vendor.bundle.base.js"></script>
<!-- endinject -->
<!-- Plugin js for this page -->
<script src="~/DesignAdman/assets/vendors/chart.js/Chart.min.js"></script>
<script src="~/DesignAdman/assets/vendors/progressbar.js/progressbar.min.js"></script>
<script src="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
<script src="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
<script src="~/DesignAdman/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
<!-- End plugin js for this page -->
<!-- inject:js -->
<script src="~/DesignAdman/assets/js/off-canvas.js"></script>
<script src="~/DesignAdman/assets/js/hoverable-collapse.js"></script>
<script src="~/DesignAdman/assets/js/misc.js"></script>
<script src="~/DesignAdman/assets/js/settings.js"></script>
<script src="~/DesignAdman/assets/js/todolist.js"></script>


<!-- endinject -->
<!-- Custom js for this page -->
<script src="~/DesignAdman/assets/js/dashboard.js"></script>
<!-- End custom js for this page -->
