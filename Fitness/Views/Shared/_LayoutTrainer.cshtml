
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Fitness Trainer</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/DesignAdman/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/DesignAdman/assets/css/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/DesignAdman/assets/images/favicon.png" />
    <link rel="shortcut icon" href="~/DesignAdman/assets/images/favicon.png" />
    <link rel="stylesheet" href="~/css/ProfileLayout.css">
</head>

<body>
    <div class="logo-container">
        <div class="logo-icon">
            <span>💪</span>
        </div>
        <div class="logo-text">Fitness</div>
    </div>
    <div class="container-scroller">
        <!-- partial:partials/_sidebar.html -->
        <nav class="sidebar sidebar-offcanvas" id="sidebar"> 
            <ul class="nav">

                
                <li class="nav-item profile">
                    <div class="profile-desc">
                        <div class="profile-pic">
                            <div class="count-indicator">
                                <img class="img-xs rounded-circle"
                                     src=@Url.Content("~/images/" + Context.Session.GetString("UserPhoto"))
                                     alt="User Photo">
                                <span class="count bg-success"></span>
                            </div>
                            <div class="profile-name">
                                <h5 class="mb-0 font-weight-normal">
                                    @Context.Session.GetString("UserNameandLastname") 
                                </h5>
                                <span>
                                    @Context.Session.GetString("UserRoleName")
                                </span>
                            </div>
                        </div>

                      
                      
                    </div>
                </li>
                <li class="nav-item menu-items">
                    @{
                        var UserID = Context.Session.GetInt32("UserID");
                    }
                    <a class="nav-link" asp-action="Details" asp-controller="ProfileUser" asp-route-id="@UserID">
                        <span class="menu-icon">
                            <i class="mdi mdi-speedometer"></i>
                        </span>
                        <span class="menu-title">MY Profile</span>
                    </a>
                </li>
                <li class="nav-item nav-category">
                    <span class="nav-link">Navigation</span>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="@Url.Action("listProfileMembar","Trainer")">
                        <span class="menu-icon">
                            <i class="mdi mdi-playlist-play"></i>
                        </span>
                        <span class="menu-title">All Member</span>
                    </a>
                </li>
                <li class="nav-item menu-items">
                    <a class="nav-link" href="@Url.Action("CreateNewPlan","Trainer")">
                        <span class="menu-icon">
                            <i class="mdi mdi-playlist-play"></i>
                        </span>
                        <span class="menu-title">Create New Plan</span>
                    </a>
                </li>
               
                 <li class="nav-item menu-items">
              
                    <a class="nav-link" href="@Url.Action("SeeAllPlan","Trainer")">
                        <span class="menu-icon">
                            <i class="mdi mdi-playlist-play"></i>
                        </span>
                        <span class="menu-title">See All Plan</span>
                    </a>
                </li>
              
            </ul>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_navbar.html -->
            <nav class="navbar p-0 fixed-top d-flex flex-row">
                <div class="navbar-brand-wrapper d-flex d-lg-none align-items-center justify-content-center">
                    <a class="navbar-brand brand-logo-mini" href="index.html"><img src="~/DesignAdman/assets/images/logo-mini.svg" alt="logo"></a>
                </div>
                <div class="navbar-menu-wrapper flex-grow d-flex align-items-stretch">
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-menu"></span>
                    </button>
                    <ul class="navbar-nav w-100">
                        <li class="nav-item w-100">
                            <form class="nav-link mt-2 mt-md-0 d-none d-lg-flex search">
                                <input type="text" class="form-control" placeholder="Search products">
                            </form>
                        </li>
                    </ul>
                    <ul class="navbar-nav navbar-nav-right">
                        <li class="nav-item dropdown d-none d-lg-block">
                            <a class="nav-link btn btn-success create-new-button" id="createbuttonDropdown" data-toggle="dropdown" aria-expanded="false" href="#">+ Create New Project</a>
                            <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="createbuttonDropdown">
                                <h6 class="p-3 mb-0">Projects</h6>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item preview-item">
                                    <div class="preview-thumbnail">
                                        <div class="preview-icon bg-dark rounded-circle">
                                            <i class="mdi mdi-file-outline text-primary"></i>
                                        </div>
                                    </div>
                                    <div class="preview-item-content">
                                        <p class="preview-subject ellipsis mb-1">Software Development</p>
                                    </div>
                                </a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item preview-item">
                                    <div class="preview-thumbnail">
                                        <div class="preview-icon bg-dark rounded-circle">
                                            <i class="mdi mdi-web text-info"></i>
                                        </div>
                                    </div>
                                    <div class="preview-item-content">
                                        <p class="preview-subject ellipsis mb-1">UI Development</p>
                                    </div>
                                </a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item preview-item">
                                    <div class="preview-thumbnail">
                                        <div class="preview-icon bg-dark rounded-circle">
                                            <i class="mdi mdi-layers text-danger"></i>
                                        </div>
                                    </div>
                                    <div class="preview-item-content">
                                        <p class="preview-subject ellipsis mb-1">Software Testing</p>
                                    </div>
                                </a>
                                <div class="dropdown-divider"></div>
                                <p class="p-3 mb-0 text-center">See all projects</p>
                            </div>
                        </li>
                    
                        <li class="nav-item nav-logout d-none d-lg-block">
                            <a class="nav-link" href="@Url.Action("loginAndRegister","Auth")">
                                <i class="mdi mdi-power"></i>
                            </a>
                        </li>
                    </ul>
                    <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                        <span class="mdi mdi-format-line-spacing"></span>
                    </button>
                </div>
            </nav>
            <!-- partial -->
            @RenderBody()
        <!-- main-panel ends -->
        </div>
    <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/DesignAdman/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/DesignAdman/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/DesignAdman/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/DesignAdman/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/DesignAdman/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/DesignAdman/assets/js/off-canvas.js"></script>
    <script src="~/DesignAdman/assets/js/hoverable-collapse.js"></script>
    <script src="~/DesignAdman/assets/js/misc.js"></script>
    <script src="~/DesignAdman/assets/js/settings.js"></script>
    <script src="~/DesignAdman/assets/js/todolist.js"></script>
 
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/DesignAdman/assets/js/dashboard.js"></script>
    <!-- End custom js for this page -->
    @await RenderSectionAsync("Scripts", required: false)
   

</body>
</html>