											var profiles = ViewBag.Profile as List<Profile>;


											
										@if (profiles != null && profiles.Any())
										{
											@foreach (var profile in profiles)
											{
											<li class="mb-2 mr-2">
												<a href="#"><img src="@Url.Content("~/images/" + profile.Photo)" alt="gym" class="sgalleryImg"></a>
											</li>
											}
										}
										else
										{
										<p>No profiles available.</p>
