<body>
	<div id="body-wrapper">

		<header id="header" class="bg-black">
			<div class="container">
				<div class="row">
					<nav class="navbar navbar-expand-lg col-md-12 padding-small">
						<div class="nav-brand">
							<a href="index.html"><img src="~/DesignHomePage/images/logo.png"></a>
						</div>
						<button class="navbar-toggler collapsed" type="button" data-toggle="collapse"
								data-target="#slide-navbar-collapse" aria-controls="slide-navbar-collapse"
								aria-expanded="false" aria-label="Toggle navigation">
							<span class="navbar-toggler-icon"><i class="icon icon-navicon"></i></span>
						</button>
						<div class="navbar-collapse collapse text-hover" id="slide-navbar-collapse">
							<ul class="navbar-nav list-inline light text-uppercase">
								<li class="nav-item active"><a asp-action="index" asp-controller="Home" class="navlink">home</a></li>
								<li class="nav-item"><a asp-action="aboutus" asp-controller="Home">about us</a></li>
								<li class="nav-item"><a asp-action="trainers" asp-controller="Home">trainers</a></li>
								<li class="nav-item"><a asp-action="Class" asp-controller="Home">class</a></li>
								<li class="nav-item"><a asp-action="blog" asp-controller="Home">blog</a></li>
								<li class="nav-item"><a asp-action="contactus" asp-controller="Home">contact us</a></li>

								@* 	<li class="nav-item"><a href="https://templatesjungle.gumroad.com/l/fitphysique-free-responsive-html-template" target="_blank" style="color:#d4000d ;">LogIn</a></li> *@
							</ul>
						</div><!--navbar-collapse-->
						<div class="action-menu">
							<div class="searchbar">
								<a href="#" class="search-button search-toggle" data-selector=".navbar">
									<i class="icon icon-search"></i>
								</a>
								<form role="search" method="get" class="search-box" action="">
									<input class="search-field text search-input" placeholder="Search" type="search">
								</form>
							</div>
							<div class="navbar-collapse collapse text-hover" id="slide-navbar-collapse">

								<ul class="navbar-nav list-inline light text-uppercase">
									<li class="dropdown">
										<a class="dropbtn">Me <iconify-icon icon="iconamoon:arrow-down-2-fill"></iconify-icon> </a>

										<div class="dropdown-content">

											 @{

												var UserID = Convert.ToDecimal(Context.Session.GetInt32("UserID"));
											 	var UserIsEnter = Context.Session.GetInt32("UserIsEnter");
											 	var UserRoleID = Context.Session.GetInt32("UserRoleID");

											 }

											 @if (UserIsEnter == 1 && UserID != null)
											 {
												<a href="@Url.Action("Details", "ProfileUser", new { id = UserID })" class="drop-item" style="color:#d4000d;">Profile</a>
											 	<a href="@Url.Action("LoginAndRegister", "Auth")" class="drop-item" style="color:#d4000d;">Logout</a>
											 }
											 else
											 {
											 	<a href="@Url.Action("LoginAndRegister", "Auth")" target="_blank" style="color:#d4000d;">LogIn</a>
											 }

										</div>
									</li>


								</ul>
							</div>
							<!--navbar-collapse--
								</div>
							</div><!--action-menu-->
					</nav>
				</div>
			</div>
		</header>

		<div>
        @RenderBody()
    </div>

